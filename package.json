{
  "name": "greyatom-ide-welcome",
  "version": "0.0.1",
  "description": "Welcome users to Commit Live with useful information",
  "main": "./lib/main",
  "repository": "https://github.com/greyatom-edu-tech/greyatom-ide-welcome",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "configSchema": {
    "showOnStartup": {
      "type": "boolean",
      "default": true,
      "description": "Show welcome panes with useful information on start up."
    }
  },
  "deserializers": {
    "SettingsView": "createSettingsView",
    "LoginView": "createLoginView"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "etch": "0.9.0"
  },
  "standard": {
    "globals": [
      "atom"
    ]
  }
}
